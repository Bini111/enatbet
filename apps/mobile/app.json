{
  "expo": {
    "name": "Enatbet",
    "slug": "enatbet",
    "scheme": "enatbet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.bini.enatbet",
          "enableGooglePay": true
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.bini.enatbet",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Take photos for your profile and listings",
        "NSPhotoLibraryUsageDescription": "Choose photos for your profile and listings",
        "NSPhotoLibraryAddUsageDescription": "Save photos to your library"
      }
    },
    "android": {
      "package": "com.bini.enatbet",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
