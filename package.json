{
  "name": "enatbet-app",
  "version": "1.0.0",
  "description": "Enatbet - Property Rental & Betting Platform",
  "author": "Enatbet Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "build:dev": "eas build --platform all --profile development",
    "build:staging": "eas build --platform all --profile staging",
    "build:prod": "eas build --platform all --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:android:dev": "eas build --platform android --profile development",
    "clean": "rm -rf node_modules package-lock.json .expo ios android && npm install",
    "clean:cache": "expo start -c",
    "clean:pods": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "prepare": "husky",
    "postinstall": "patch-package",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "update:deps": "npx npm-check-updates -u && npm install",
    "security:check": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-firebase/analytics": "20.5.0",
    "@react-native-firebase/app": "20.5.0",
    "@react-native-firebase/auth": "20.5.0",
    "@react-native-firebase/crashlytics": "20.5.0",
    "@react-native-firebase/firestore": "20.5.0",
    "@react-native-firebase/functions": "20.5.0",
    "@react-native-firebase/storage": "20.5.0",
    "@sentry/react-native": "~6.10.0",
    "@stripe/stripe-react-native": "0.38.6",
    "date-fns": "3.6.0",
    "expo": "~52.0.0",
    "expo-apple-authentication": "~7.1.0",
    "expo-application": "~6.0.0",
    "expo-auth-session": "~6.0.0",
    "expo-av": "~14.0.0",
    "expo-blur": "~13.0.0",
    "expo-build-properties": "~0.13.0",
    "expo-calendar": "~13.0.0",
    "expo-constants": "~17.0.0",
    "expo-crypto": "~13.0.0",
    "expo-dev-client": "~5.0.0",
    "expo-device": "~7.0.0",
    "expo-haptics": "~13.0.0",
    "expo-image": "~2.0.0",
    "expo-image-manipulator": "~12.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-linking": "~7.0.0",
    "expo-location": "~18.0.0",
    "expo-notifications": "~0.29.0",
    "expo-router": "~4.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.0",
    "expo-updates": "~0.26.0",
    "expo-web-browser": "~14.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-google-places-autocomplete": "2.5.6",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-maps": "1.18.0",
    "react-native-mmkv": "3.0.2",
    "react-native-modal": "13.0.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.19.13",
    "sentry-expo": "^7.2.0",
    "zod": "3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "lint-staged": "^15.2.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx",
      "!src/**/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
